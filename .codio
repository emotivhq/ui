{
// Configure your Run and Preview buttons here.

// Run button configuration
 "commands": {
      "----------------------App Tasks:----------------------": "node --version",
	 "Build Dev": "cd ~/workspace/grunt ; grunt dev",
	 "Build Release": "cd ~/workspace/grunt ; grunt build",
     "Start Web App SSL": "dev_appserver.py --port=9500 --host=0.0.0.0 ./",
     "Start Web App": "dev_appserver.py --port=3000 --host=0.0.0.0 ./",  
      "---------------------": "node --version",
	 "Push to GAE: Dev": "echo $GAE_PASSWORD | appcfg.py update app.yaml --email=$GAE_EMAIL --passin",
	 "Push to GAE: Production": "echo $GAE_PASSWORD | appcfg.py update app.production.yaml --email=$GAE_EMAIL --passin",
      "----------------Environment Tasks:----------------": "node --version",
     "Setup ENV": "export PKG_CONFIG_PATH=/usr/local/lib/pkgconfig", 
      "Start SSH agent": "ssh-agent -s",
      "List SSH fingerprints": "ssh-add -l",
      "Test SSH to Github": "ssh -vT git@github.com",
      "Turn on & cache with credential helper": "git config --global credential.helper 'cache --timeout=3600'",  
      "Github oAuth Token": "<enter your own token> (Get Help: http://bit.ly/1Ep65b8)",
      "Node version": "node --version"
 },

// Preview button configuration
 "preview": {
       "Preview SSL": "https://{{domain}}:9500/",
       "Preview": "http://{{domain}}:3000/",
       "Preview 3001": "http://{{domain}}:3001/",
       "Ungit": "http://{{domain}}:8448/#/repository?path=%2Fhome%2Fcodio%2Fworkspace",
       "Project Index (static)": "https://{{domain}}/{{index}}",
       "Current File (static)": "https://{{domain}}/{{filepath}}"
 }
}